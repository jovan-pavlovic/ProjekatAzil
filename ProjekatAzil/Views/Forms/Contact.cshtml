@model ProjekatAzil.Models.Form
@section HeaderBottom{
    <div class="about-heading">
        <div class="container">
            <h2>Contact Form</h2>
        </div>
    </div>
}
<div class="container">
    <div class="s1">
    @using (Html.BeginForm("Contact", "Forms", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <br />
        <br />
        <p>First name:</p>
        <input type="text" name="Model.FirstName" value="">
        <br />
        <br />
        <p>Last name:</p>
        <input type="text" name="Model.LastName" value="">
        <br />
        <br />
        <p>Phone number:</p>
        <input type="text" name="Model.PhoneNumber" value="">
        <br />
        <br />
        <p>E-mail:</p>
        <input type="email" name="Model.Email">
        <br />
        <br />
        <p>Comment:</p>
        <textarea rows="5" cols="60">
    </textarea>
        <br />
        <br />
        <input type="submit" value="Submit">
        <br />
        <br />
    }
        </div>
</div>


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}